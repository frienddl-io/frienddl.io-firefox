{
  "name": "frienddl.io - skribbl.io friend finder",
  "short_name": "frienddl.io",
  "version": "1.1.4",
  "manifest_version": 2,
  "description": "Find friends in public rooms on skribbl.io",
  "browser_action": {
    "default_popup": "html/default.html",
    "default_title": "frienddl.io"
  },
  "content_scripts": [
    {
      "matches": ["https://skribbl.io/"],
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "img/favicon/favicon-16x16.png",
    "32": "img/favicon/favicon-32x32.png",
    "48": "img/favicon/favicon.ico",
    "96": "img/favicon/favicon-96x96.png",
    "128": "img/favicon/favicon-128x128.png"
  },
  "permissions": [
    "storage"
  ]
}
